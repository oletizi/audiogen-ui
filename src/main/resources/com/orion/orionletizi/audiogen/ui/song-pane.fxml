<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<GridPane hgap="10.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
          prefHeight="659.0" prefWidth="600.0" vgap="10.0" xmlns="http://javafx.com/javafx/8"
          xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.orionletizi.audiogen.ui.controller.SongPaneController">
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    <ColumnConstraints hgrow="NEVER"/>
  </columnConstraints>
  <rowConstraints>
    <RowConstraints vgrow="NEVER" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="NEVER"/>
    <RowConstraints maxHeight="389.0" minHeight="263.0" prefHeight="332.0" vgrow="SOMETIMES"/>
    <RowConstraints maxHeight="126.0" minHeight="0.0" prefHeight="0.0" vgrow="SOMETIMES"/>
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
  </rowConstraints>
   <padding>
      <Insets left="10.0" right="10.0" top="10.0" />
   </padding>
   <children>
      <VBox prefWidth="100.0">
         <children>
           <TextField fx:id="songPathDisplay" editable="false"/>
         </children>
      </VBox>
      <HBox spacing="10.0" GridPane.columnIndex="1">
         <children>
            <Button fx:id="openSongButton" mnemonicParsing="false" text="Open" />
            <Button fx:id="newSongButton" mnemonicParsing="false" text="New" />
         </children>
      </HBox>
     <HBox spacing="10.0" GridPane.columnIndex="1" GridPane.rowIndex="3">
         <children>
            <Button fx:id="saveSongButton" mnemonicParsing="false" text="Save Song" />
         </children>
      </HBox>
     <TextField fx:id="songPathField" text="Enter song path..." GridPane.rowIndex="1"/>
     <Button fx:id="generateButton" mnemonicParsing="false" text="Generate" GridPane.columnIndex="1"
             GridPane.rowIndex="4"/>
     <HBox id="audioPlayerBox" prefHeight="100.0" prefWidth="200.0" GridPane.rowIndex="4">
       <children>
         <fx:include fx:id="audioPlayer" source="audio-player.fxml"/>
       </children>
     </HBox>
     <Pane fx:id="beatInstrumentIncludePane" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="0"
           GridPane.rowIndex="2" GridPane.columnSpan="1">
       <children>
         <fx:include fx:id="beatInstrumentPane" source="beat-instrument-pane.fxml"/>
       </children>
     </Pane>
   </children>
</GridPane>
