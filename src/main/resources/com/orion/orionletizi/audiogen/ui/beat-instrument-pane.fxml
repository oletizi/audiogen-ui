<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>

<GridPane hgap="10.0" vgap="10.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.orionletizi.audiogen.ui.controller.BeatInstrumentPaneController">
  <children>
    <TextField fx:id="instrumentPathDisplay" />
      <HBox spacing="10.0" GridPane.columnIndex="1">
         <children>
            <Button fx:id="openInstrumentButton" mnemonicParsing="false" text="Open" />
            <Button fx:id="newInstrumentButton" mnemonicParsing="false" text="New" />
         </children>
      </HBox>
      <VBox prefHeight="200.0" prefWidth="100.0" GridPane.rowIndex="1">
         <children>
            <Separator prefWidth="200.0" />
            <Label text="Sampler Program" />
         </children>
      </VBox>
      <TextField fx:id="programPathDisplay" GridPane.rowIndex="2" />
      <Button fx:id="chooseProgramButton" mnemonicParsing="false" text="Choose" GridPane.columnIndex="1" GridPane.rowIndex="2" />
      <Button fx:id="saveInstrumentButton" mnemonicParsing="false" text="Save" GridPane.columnIndex="1" GridPane.rowIndex="3" GridPane.valignment="TOP" />
  </children>
  <padding>
    <Insets left="10.0" right="10.0" top="10.0" />
  </padding>
   <columnConstraints>
      <ColumnConstraints />
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints maxHeight="193.0" minHeight="21.0" prefHeight="21.0" vgrow="NEVER" />
      <RowConstraints maxHeight="206.0" minHeight="32.0" prefHeight="32.0" />
      <RowConstraints maxHeight="204.0" minHeight="10.0" prefHeight="204.0" />
   </rowConstraints>
</GridPane>
