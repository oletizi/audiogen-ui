                 <ScrollPane prefHeight="273.0">
                     <content>
                       <ListView fx:id="chordalInstrumentMidiPatternList" prefHeight="271.0" prefWidth="400.0" />
                     </content>
                  </ScrollPane>
                  <HBox prefWidth="200.0" spacing="10.0">
                     <children>
                       <Button fx:id="addChordalInstrumentMidiPatternButton" mnemonicParsing="false" text="Add" />
