                   <content>
                     <ListView fx:id="chordalInstrumentVariantsListView" prefHeight="98.0" prefWidth="452.0" />
                   </content>
                 </ScrollPane>
                 <HBox spacing="10.0" GridPane.rowIndex="1">
                   <children>
                     <Button fx:id="addChordalInstrumentVariantsButton" mnemonicParsing="false" text="Add" />
                     <Button fx:id="deleteChordalInstrumentVariantsButton" mnemonicParsing="false" text="Delete" />
                   </children>
                 </HBox>
                  <Label text="Variants" />
