               <fx:include fx:id="audioPlayer" source="player.fxml" />
             </children>
           </HBox>
            <VBox fx:id="chordalInstrumentsChooserBox" prefHeight="200.0" prefWidth="100.0" spacing="10.0" GridPane.rowIndex="3">
