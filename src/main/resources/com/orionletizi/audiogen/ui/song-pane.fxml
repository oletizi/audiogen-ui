<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<ScrollPane prefHeight="800.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.orionletizi.audiogen.ui.controller.SongPaneController">
   <content>
      <GridPane hgap="10.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="1000.0" prefWidth="900.0" vgap="10.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="566.0" minWidth="10.0" prefWidth="448.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="410.0" prefWidth="410.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints vgrow="NEVER" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="NEVER" />
          <RowConstraints maxHeight="348.0" minHeight="201.0" prefHeight="338.0" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="400.0" minHeight="174.0" prefHeight="324.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="126.0" minHeight="0.0" prefHeight="0.0" vgrow="NEVER" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <padding>
            <Insets left="10.0" right="10.0" top="10.0" />
         </padding>
         <children>
            <VBox prefWidth="100.0">
               <children>
                 <TextField fx:id="songPathDisplay" editable="false" />
               </children>
            </VBox>
            <HBox spacing="10.0" GridPane.columnIndex="1">
               <children>
                  <Button fx:id="openSongButton" mnemonicParsing="false" text="Open" />
                  <Button fx:id="newSongButton" mnemonicParsing="false" text="New" />
               </children>
            </HBox>
           <HBox spacing="10.0" GridPane.columnIndex="1" GridPane.rowIndex="4">
               <children>
                  <Button fx:id="saveSongButton" mnemonicParsing="false" text="Save Song" />
                  <Button fx:id="installSongButton" mnemonicParsing="false" text="Install Song" />
               </children>
            </HBox>
           <TextField fx:id="songPathField" text="Enter song path..." GridPane.rowIndex="1" />
           <Button fx:id="generateButton" mnemonicParsing="false" text="Generate" GridPane.columnIndex="1" GridPane.rowIndex="5" GridPane.valignment="TOP">
               <GridPane.margin>
                  <Insets top="10.0" />
               </GridPane.margin>
            </Button>
           <HBox id="audioPlayerBox" prefHeight="100.0" prefWidth="200.0" GridPane.rowIndex="5" GridPane.valignment="TOP">
