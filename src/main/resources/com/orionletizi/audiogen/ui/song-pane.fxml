               <Label text="Midi Patterns" />
               <ScrollPane prefHeight="240.0" prefWidth="200.0">
                 <content>
                   <ListView fx:id="beatInstrumentVariantsMidPatternsListView" prefHeight="217.0" prefWidth="400.0" />
                 </content>
               </ScrollPane>
               <HBox prefHeight="100.0" prefWidth="200.0" spacing="10.0">
                 <children>
                   <Button fx:id="addBeatInstrumentMidiPatternButton" mnemonicParsing="false" text="Add" />
                   <Button fx:id="deleteBeatInstrumentMidiPatternButton" mnemonicParsing="false" text="Delete" />
                 </children>
               </HBox>
             </children>
           </VBox>
           <VBox prefHeight="200.0" prefWidth="100.0" spacing="10.0" GridPane.rowIndex="2">
             <children>
               <Separator prefWidth="200.0" />
               <Label text="Beat Instrument Variants" />
                  <HBox alignment="CENTER_LEFT" spacing="10.0">
                     <children>
                        <Label text="Path:" />
                        <TextField fx:id="beatInstrumentPathField" prefHeight="27.0" prefWidth="412.0" />
                     </children>
                  </HBox>
               <ScrollPane prefHeight="200.0" prefWidth="200.0">
                 <content>
                   <ListView fx:id="beatInstrumentVariantsListView" prefHeight="197.0" prefWidth="438.0" />
                 </content>
               </ScrollPane>
               <HBox prefHeight="100.0" prefWidth="200.0" spacing="10.0">
                 <children>
                   <Button fx:id="addBeatInstrumentVariantsButton" mnemonicParsing="false" text="Add" />
                   <Button fx:id="deleteBeatInstrumentVariantsButton" mnemonicParsing="false" text="Delete" />
                        <Button fx:id="installBeatInstrumentsButton" mnemonicParsing="false" text="Install" />
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
      </GridPane>
   </content>
</ScrollPane>
