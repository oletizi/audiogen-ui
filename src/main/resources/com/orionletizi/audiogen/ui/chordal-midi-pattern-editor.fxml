<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>
<ScrollPane prefHeight="800.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.orionletizi.audiogen.ui.controller.ChordalMidiPatternEditorController">
  <content>
    <VBox spacing="10.0">
      <children>
        <Separator prefWidth="200.0"/>
        <Label text="Midi File"/>
        <TextField fx:id="midiFileField" editable="false"/>
        <VBox fx:id="chordalPatternEditorIncludeBox">
          <children>
            <fx:include fx:id="chordalPatternEditor" source="chordal-pattern-editor.fxml"/>
          </children>
        </VBox>
        <VBox fx:id="auditionBox" prefHeight="200.0" prefWidth="100.0" spacing="10.0">
          <children>
            <Separator prefWidth="200.0"/>
            <Label text="Audition"/>
          </children>
        </VBox>
      </children>
    </VBox>
  </content>
</ScrollPane>
